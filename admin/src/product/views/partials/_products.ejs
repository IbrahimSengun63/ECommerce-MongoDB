<% products.forEach(product=> { %>

    <tbody>

        <tr>

            <td>
                <%-product.productName%>
            </td>
            <td>
                <%-product.category%>
            </td>
            <td>
                <%-product.description%>
            </td>
            <td>
                <%-product.price%>
            </td>
            <td>
                <%-product.discount%>
            </td>
            <td>
                <%-product.netPrice%>
            </td>
            <td>
                <%-product.stock%>
            </td>
            <td>
                <%-product.shop%>
            </td>
            <td>
                <%-product.image%>
            </td>
            <td>
                <form action="/admin/product/edit" method="POST">
                    <input type=hidden name=id value="<%=product._id%>">
                    <p data-placement="top" data-toggle="tooltip" title="Edit"><button type="submit"
                            class="btn btn-primary btn-xs" data-title="Edit" data-toggle="modal"><span
                                class="glyphicon glyphicon-pencil"></span></button></p>
                </form>

            </td>
            <td>
                <form action="/admin/product/delete" method="POST">
                    <input type=hidden name=id value="<%=product._id%>">
                    <p data-placement="top" data-toggle="tooltip" title="Delete"><button type="submit" class="btn btn-danger btn-xs"
                            data-title="Delete" data-toggle="modal" data-target="#delete"><span
                                class="glyphicon glyphicon-trash"></span></button></p>
                </form>
            </td>
        </tr>

    </tbody>
    <%}); %>